<!-- Podcasts Section -->
<mj-section background-color="#ffffff" padding="20px">
  <mj-wrapper
    background-color="#2069FA"
    padding="20px"
    padding-top="20px"
    border-radius="16px"
  >
    <mj-column padding="10px" width="40%" vertical-align="top">
      <mj-image
        border-radius="16px"
        width="200px"
        height="200px"
        background-color="#091D2C"
        padding="0px"
